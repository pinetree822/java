package co.kr.team.sports;

public class SportsDto2 {
	private String h_day;
	private String id;
	private double sports;
	
	
	
	public String getH_day() {
		return h_day;
	}
	public void setH_day(String h_day) {
		this.h_day = h_day;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public double getSports() {
		return sports;
	}
	public void setSports(double sports) {
		this.sports = sports;
	}
	
	@Override
	public String toString() {
		return "SportsDto2 [h_day=" + h_day + ", id=" + id + ", sports="
				+ sports + "]";
	}
}
