package co.kr.team.food;
public class KcalDto { 
	private int no;
	private String fname;
	private int kcal;
	private String h_day;
	private String id;
	private int totalkcal;
	
	// ¿ìÅ¬¸¯ -> Source -> Generate Getters and Setters -> Select All -> OK
	
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public String getFname() {
		return fname;
	}
	public void setFname(String fname) {
		this.fname = fname;
	}
	
	public String getH_day() {
		return h_day;
	}
	public void setH_day(String h_day) {
		this.h_day = h_day;
	}
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public int getKcal() {
		return kcal;
	}
	public void setKcal(int kcal) {
		this.kcal = kcal;
	}
	
	public int getTotalkcal() {
		return totalkcal;
	}
	public void setTotalkcal(int totalkcal) {
		this.totalkcal = totalkcal;
	}
	@Override
	public String toString() {
		return "KcalDto [no=" + no + ", fname=" + fname + ", kcal=" + kcal
				+ ", h_day=" + h_day + ", id=" + id + ", totalkcal="
				+ totalkcal + "]";
	}

	
}//end









