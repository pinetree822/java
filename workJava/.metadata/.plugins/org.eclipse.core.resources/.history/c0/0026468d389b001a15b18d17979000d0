package net.tis.day21;

public class Test {

	public static void main(String[] args) {
		// Test.java 출력연습
		String title="게시판";
		int age = 37;
		double avg = 78.9;
		char grade ='B';
		
		System.out.println("제목=" + title);
		System.out.println("나이=" + age);
		System.out.println("평균=" + avg);
		System.out.println("학점=" + grade);
		
	}

}//class End
